        android:name="com.example.covidapp.ui.symptoms.SymptomsFragment"
        android:label="fragment_symptoms"
        tools:layout="@layout/fragment_symptoms" />
    <fragment
        android:id="@+id/preventationFragment"
        android:name="com.example.covidapp.ui.preventation.PreventationFragment"
        android:label="fragment_preventation"
        tools:layout="@layout/fragment_preventation" />
    <fragment
        android:id="@+id/articleFragment"
        android:name="com.example.covidapp.ui.article.ArticleFragment"
        android:label="fragment_article"
        tools:layout="@layout/fragment_article" />
    <fragment
        android:id="@+id/helpFragment"
        android:name="com.example.covidapp.ui.help.HelpFragment"
        android:label="fragment_help"
        tools:layout="@layout/fragment_help" />
    <fragment
        android:id="@+id/newsFragment"
        android:name="com.example.covidapp.ui.news.NewsFragment"
        android:label="fragment_news"
